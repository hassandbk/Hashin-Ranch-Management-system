<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="422.0" prefWidth="974.0"  xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml">
    <!-- Other UI elements can be added here -->

    <HBox AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="1.0" AnchorPane.topAnchor="11.0">

    <!-- ScrollPane containing TableView -->
    <TableView fx:id="healthInfoTableView" HBox.hgrow="ALWAYS">

        <columns>

            <!-- Respiratory Rate Column -->
            <TableColumn fx:id="respiratoryRateColumn" prefWidth="106.39999234676361" text="Respiratory Rate">
                <cellValueFactory>
                    <PropertyValueFactory property="respiratoryRate" />
                </cellValueFactory>
            </TableColumn>

            <!-- Fertility Status Column -->
            <TableColumn fx:id="fertilityStatusColumn" prefWidth="104.8000259399414" text="Fertility Status">
                <cellValueFactory>
                    <PropertyValueFactory property="fertilityStatus" />
                </cellValueFactory>
            </TableColumn>

            <!-- Last Health Checkup Column -->
            <TableColumn fx:id="lastHealthCheckupColumn" prefWidth="136.0" text="Last Health Checkup">
                <cellValueFactory>
                    <PropertyValueFactory property="lastHealthCheckup" />
                </cellValueFactory>
            </TableColumn>

            <!-- Activity Level Column -->
            <TableColumn fx:id="activityLevelColumn" minWidth="2.39996337890625" prefWidth="95.199951171875" text="Activity Level">
                <cellValueFactory>
                    <PropertyValueFactory property="activityLevel" />
                </cellValueFactory>
            </TableColumn>

            <!-- Illness History Column -->
            <TableColumn fx:id="illnessHistoryColumn" prefWidth="190.39999389648438" text="Illness History">
                <cellValueFactory>
                    <PropertyValueFactory property="illnessHistory" />
                </cellValueFactory>
            </TableColumn>

            <!-- Parasite Control Column -->
            <TableColumn fx:id="parasiteControlColumn" prefWidth="101.5999755859375" text="Parasite Control">
                <cellValueFactory>
                    <PropertyValueFactory property="parasiteControl" />
                </cellValueFactory>
            </TableColumn>

            <!-- Next Checkup Due Column -->
            <TableColumn fx:id="nextCheckupDueColumn" minWidth="3.20001220703125" prefWidth="112.800048828125" text="Next Checkup Due">
                <cellValueFactory>
                    <PropertyValueFactory property="nextCheckupDue" />
                </cellValueFactory>
            </TableColumn>

            <!-- Pregnancy Status Column -->
            <TableColumn fx:id="pregnancyStatusColumn" minWidth="0.0" prefWidth="113.60009765625" text="Pregnancy Status">
                <cellValueFactory>
                    <PropertyValueFactory property="pregnancyStatus" />
                </cellValueFactory>
            </TableColumn>

        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>

        <!-- Add more columns as needed -->


    </TableView>


    </HBox>


</AnchorPane>
